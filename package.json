{
  "name": "generator-prototype-subtheme",
  "version": "0.0.8",
  "description": "Generate a subtheme for the Prototype Drupal base theme.",
  "homepage": "",
  "author": {
    "name": "John Ferris",
    "email": "john@atendesigngroup.com",
    "url": "https://aten.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Drupal",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "1.19.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^3.6.0",
    "yeoman-test": "^6.2.0"
  },
  "engines": {
    "node": ">=0.12",
    "npm": ">= 6.1.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^5.4.2",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "atendesigngroup/generator-prototype-subtheme",
  "license": "MIT"
}
